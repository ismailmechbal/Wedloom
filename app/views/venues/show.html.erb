<p id="notice"><%= notice %></p>

<center>

<h1>
  <%= @venue.name %>
</h1>

<h3>
  <%= @venue.description %>
</h3>

<!--
<p>
  <b>Supporting images:</b>
  <%= @venue.supporting_images %>
</p>

-->

<p>
  <b>Maximum Capacity:</b>
  <%= number_with_delimiter(@venue.capacity, :delimiter => ',') %>
</p>

<p>
  <b>Rental cost:</b>
  $<%= number_with_delimiter(@venue.rental_cost, :delimiter => ',') %>
</p>

<p>
  <b>Cost per plate:</b>
  $<%= number_with_delimiter(@venue.cost_per_plate, :delimiter => ',') %>
</p>

<p>
  <b>Minimum cost:</b>
  $<%= number_with_delimiter(@venue.minimum, :delimiter => ',') %>
</p>


<p>
  <b>Tier:</b>
  <%= @venue.tier %>
</p>



<p>
  <b>Tags:</b>
  <%= @venue.tags %>
</p>



<%= image_tag(@venue.image) %>
  </br>
